   "source": [
    "fib = [0, 1]\n",
    "file=open(\"fib.txt\",\"w\")\n",
    "num_times = int(input(\"Please enter a number\\t\"))\n",
    "count = 0\n",
    "while count < num_times:\n",
    "    num = fib[0]+fib[1]\n",
    "    file.write(\"%s\\n\" % fib[0])\n",
    "    fib.append(num)\n",
    "    fib.pop(0)\n",
    "    count = count+1\n",
    "file.close()"
   ]
  }
